# [params = COMPANION_DATADIR]
# Full path to the directory to which provider files will be downloaded temporarily.
COMPANION_DATADIR=

# [params = COMPANION_SECRET]
# A secret string which Companion uses to generate authorization tokens.
# You should generate a long random string for this (for example, using crypto.randomBytes(64).toString('hex')).
COMPANION_SECRET=

# [params = COMPANION_SECRET_FILE]
# An absolute path to a file containing only the secret.
COMPANION_SECRET_FILE=

# [params = COMPANION_PREAUTH_SECRET]
# A strong, randomly generated secret for the Transloadit companionKeysParams feature.
# Do not use the same secret as COMPANION_SECRET.
COMPANION_PREAUTH_SECRET=

# [params = COMPANION_PREAUTH_SECRET_FILE]
# An absolute path to a file containing only the preauth secret.
COMPANION_PREAUTH_SECRET_FILE=

# [params = COMPANION_UPLOAD_URLS]
# An allowlist (array) of strings (exact URLs) or regular expressions.
# Companion will only accept uploads to these URLs.
COMPANION_UPLOAD_URLS=http://localhost:8081/upload,https://uploadpilot-compup.onrender.com/upload

# [params = COMPANION_PORT]
# The port on which to start the standalone server (defaults to 3020).
COMPANION_PORT=3020

# [params = COMPANION_PROTOCOL]
# Used to build a URL referencing the Companion instance (http or https).
# Use http for local development; set to https in production behind an SSL proxy.
COMPANION_PROTOCOL=https

# [params = COMPANION_DOMAIN]
# Your server’s publicly facing hostname (e.g., example.com).
COMPANION_DOMAIN=uploadpilot-compup.onrender.com

# [params = COMPANION_OAUTH_DOMAIN]
# A fixed subdomain/server to handle OAuth authentication when using multiple Companion instances.
COMPANION_OAUTH_DOMAIN=uploadpilot-compup.onrender.com

# [params = COMPANION_PATH]
# The server path where the Companion app is mounted (e.g., /companion).
COMPANION_PATH=

# [params = COMPANION_IMPLICIT_PATH]
# If the URL’s path in your reverse proxy is different from your Companion path in your express app,
# set this to the path defined in your reverse proxy.
# For example, if your Companion URL is example.com/mypath/companion,
# and /mypath is defined in NGINX while /companion is used by your app, set this to /mypath.
COMPANION_IMPLICIT_PATH=

# [params = COMPANION_DOMAINS]
# A JSON array of valid hosts for OAuth validation.
# Example: ["sub2.example.com", "sub3.example.com", "(\\w+).example.com"]
COMPANION_DOMAINS=uploadpilot-compup.onrender.com

# [params = COMPANION_SELF_ENDPOINT]
# Typically set to server.host + server.path; this value is added as the "i-am" header in every response.
COMPANION_SELF_ENDPOINT=uploadpilot-compup.onrender.com/remote

# [params = COMPANION_COOKIE_DOMAIN]
# Customize the domain of the cookies created for Express sessions (standalone-only).
COMPANION_COOKIE_DOMAIN=

# [params = COMPANION_HIDE_WELCOME]
# Set to true to disable the welcome message shown at '/' (standalone-only).
COMPANION_HIDE_WELCOME=true

# --- CORS and Client Origins ---

